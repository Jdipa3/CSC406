package asgn01;

public class ALNode {
	
	private Node theNode;
	private int weight;
	private ALNode nextNode;
	
	public ALNode(Node node, int w, ALNode nN){
		this.theNode = node;
		this.weight = w;
		this.nextNode = nN;
	}
	
	public int getWeight(){
		return weight;
	}
	
	public Node getNode(){
		return theNode;
	}
		
	public ALNode getNextNode(){
		return nextNode;
	}
	
	public void setNextNode(ALNode node){
		this.nextNode = node;
	}
}
